{
  "expo": {
    "name": "FoodLog",
    "slug": "FoodLog",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "native",
      "backgroundColor": "#a8c480"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#a8c480",
          "image": "./assets/splash-icon.png",
          "imageWidth": 100
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#a8c480"
      },
      "package": "com.astrojoni89.FoodLog"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "95492ebe-52e7-4916-b4bb-e8921f5ff190"
      }
    },
    "owner": "astrojoni89"
  }
}
